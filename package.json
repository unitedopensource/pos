{
  "name": "unitedpos",
  "version": "0.8.52",
  "description": "UnitedPOS point of sale system",
  "author": "Xuewu Huang <UnitedPOS.service@gmail.com>",
  "license": "Apache-2.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w serialport"
  },
  "build": {
    "productName": "UnitedPOS",
    "appId": "unitedpos.software.pos.beta",
    "copyright": "Copyright Â© 2017 United POS Inc.",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.1.1",
    "axios": "^0.17.1",
    "chart.js": "^2.7.1",
    "diskusage": "^0.2.4",
    "file-saver": "^1.3.3",
    "getmac": "^1.2.1",
    "hammerjs": "^2.0.8",
    "lodash.debounce": "^4.0.8",
    "md5": "^2.2.1",
    "moment": "^2.20.1",
    "net": "^1.0.2",
    "nodemailer": "^4.4.1",
    "serialport": "^4.0.7",
    "signature_pad": "^2.3.2",
    "v-mask": "^1.3.0",
    "vue": "^2.5.13",
    "vue-chartjs": "^3.0.2",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.8.1",
    "vue-socket.io": "^2.1.1",
    "vue-touch": "^2.0.0-beta.4",
    "vuebars": "^0.1.1",
    "vuedraggable": "^2.16.0",
    "vuetrend": "^0.2.3",
    "vuex": "^2.5.0",
    "wake_on_lan": "0.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.50.0",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "multispinner": "^0.2.1",
    "node-gyp": "^3.6.2",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0"
  }
}
